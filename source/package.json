{
  "name": "express-ws",
  "version": "1.0.0",
  "description": "tdd with express",
  "main": "app,js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon index",
    "test": "cross-env NODE_ENV=test jest --watch --verbose",
    "posttest": "cross-env NODE_ENV=test node test-cleanup",
    "lint": "eslint .",
    "test:staging": "cross-env NODE_ENV=staging jest -f . --runInBand",
    "migrate:test": "cross-env NODE_ENV=staging sequelize-cli db:migrate",
    "pretest:staging": "npm run migrate:test"
  },
  "author": "Phuc Le",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "file-type": "^16.5.3",
    "i18next": "^20.3.5",
    "i18next-fs-backend": "^1.1.1",
    "i18next-http-middleware": "^3.1.4",
    "nodemailer": "^6.6.3",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "nodemailer-stub": "^1.2.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "smtp-server": "^3.9.0",
    "supertest": "^6.1.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "eqeqeq": "warn",
      "prettier/prettier": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "jest": {
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/config/"
    ]
  }
}
